{
  "version": 2,
  "builds": [
    {
      "src": "flask_dashboard.py",
      "use": "@vercel/python"
    },
    {
      "src": "templates/*",
      "use": "@vercel/static"
    },
    {
      "src": "static/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/templates/(.*)",
      "dest": "/templates/$1"
    },
    {
      "src": "/(.*)",
      "dest": "flask_dashboard.py"
    }
  ]
}